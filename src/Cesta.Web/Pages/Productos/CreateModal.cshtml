@page
@using Cesta.Localization
@using Cesta.Web.Pages.Productos
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<CestaResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Producto" asp-page="/Productos/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewProducto"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>